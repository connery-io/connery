{
  "name": "connery",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "yarn run dev",
    "dev": "dotenv -- turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "publish-packages": "turbo run build && changeset version && changeset publish"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "turbo": "latest",
    "@changesets/cli": "^2.26.2"
  }
}
